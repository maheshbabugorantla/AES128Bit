// Description: "This is used to implement the Byte Substitution Layer"

module SBox
(
	input wire [127:0] inputData,
	input wire enable,
	output wire [127:0] byteSubData
);

	reg [127:0] tempByteSubData

	genvar index;

	generate
		for(index = 0; index <= 120; index = index + 8)
		begin
			sbox_unit SBOXUNIT1 (.dataIn(inputData[index + 7 : index]), .byteSOut(tempByteSubData[index + 7 : index]));
		end
	endgenerate
	
	assign byteSubData = (enable == 1) ? tempByteSubData : inputData;

endmodule 